{
  "manifest_version": 2,
  "name": "Notebook",
  "version": "1.0",
  "description": "Save your handy notes with a good looking and easy to navigate through notebook",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon16.png",
    "default_title": "Notebook",
    "browser_style": true
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}